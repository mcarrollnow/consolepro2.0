document.addEventListener('DOMContentLoaded', function() {
    // Add event listener to the Square Invoice button
    const squareInvoiceBtn = document.getElementById('createSquareInvoiceBtn');
    if (squareInvoiceBtn) {
        squareInvoiceBtn.addEventListener('click', async function() {
            const formData = collectFormData();
            await createSquareInvoice(formData);
        });
    }
